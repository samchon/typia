{
  "name": "typia",
  "version": "12.0.0-dev.20260225",
  "description": "Superfast runtime validators with only one line",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./lib/transform": "./src/transform.ts",
    "./lib/internal/*.js": "./src/internal/*.ts",
    "./src/internal/*": "./src/internal/*.ts",
    "./package.json": "./package.json"
  },
  "bin": {
    "typia": "./lib/executable/typia.js"
  },
  "tsp": {
    "tscOptions": {
      "parseAllJsDoc": true
    }
  },
  "scripts": {
    "build": "rimraf lib && tsc && rollup -c && cp ../../README.md README.md",
    "dev": "rimraf lib && tsc --watch",
    "prepack": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samchon/typia"
  },
  "author": "Jeongho Nam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samchon/typia/issues"
  },
  "homepage": "https://typia.io",
  "dependencies": {
    "@standard-schema/spec": "^1.0.0",
    "@typia/core": "workspace:^",
    "@typia/interface": "workspace:^",
    "@typia/transform": "workspace:^",
    "@typia/utils": "workspace:^",
    "commander": "^10.0.0",
    "comment-json": "^4.2.3",
    "inquirer": "^8.2.5",
    "package-manager-detector": "^0.2.0",
    "randexp": "^0.5.3"
  },
  "peerDependencies": {
    "typescript": ">=4.8.0 <5.10.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "catalog:rollup",
    "@rollup/plugin-node-resolve": "catalog:rollup",
    "@rollup/plugin-typescript": "catalog:rollup",
    "@types/inquirer": "^8.2.5",
    "@types/node": "catalog:utils",
    "@types/ts-expose-internals": "catalog:typescript",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "chalk": "^4.0.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "rimraf": "catalog:utils",
    "rollup": "catalog:rollup",
    "rollup-plugin-auto-external": "catalog:rollup",
    "rollup-plugin-node-externals": "catalog:rollup",
    "suppress-warnings": "^1.0.2",
    "tinyglobby": "^0.2.12",
    "ts-node": "catalog:typescript",
    "typescript": "catalog:typescript"
  },
  "sideEffects": false,
  "files": [
    "LICENSE",
    "README.md",
    "package.json",
    "lib",
    "src"
  ],
  "keywords": [
    "fast",
    "json",
    "stringify",
    "typescript",
    "transform",
    "ajv",
    "io-ts",
    "zod",
    "schema",
    "json-schema",
    "generator",
    "assert",
    "clone",
    "is",
    "validate",
    "equal",
    "runtime",
    "type",
    "typebox",
    "checker",
    "validator",
    "safe",
    "parse",
    "prune",
    "random",
    "protobuf",
    "llm",
    "llm-function-calling",
    "structured-output",
    "openai",
    "chatgpt",
    "claude",
    "gemini",
    "llama"
  ],
  "packageManager": "pnpm@10.6.4",
  "publishConfig": {
    "access": "public",
    "types": "lib/index.d.ts",
    "main": "lib/index.js",
    "module": "lib/index.mjs",
    "exports": {
      ".": {
        "types": "./lib/index.d.ts",
        "import": "./lib/index.mjs",
        "default": "./lib/index.js"
      },
      "./lib/transform": {
        "types": "./lib/transform.d.ts",
        "import": "./lib/transform.mjs",
        "default": "./lib/transform.js"
      },
      "./lib/internal/*.js": {
        "types": "./lib/internal/*.d.ts",
        "import": "./lib/internal/*.mjs",
        "default": "./lib/internal/*.js"
      },
      "./package.json": "./package.json"
    }
  }
}