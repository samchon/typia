{
  "$ref": "#/$defs/ObjectPrimitive.IArticle",
  "$defs": {
    "ObjectPrimitive.IArticle": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "extension": {
          "oneOf": [
            {
              "const": "txt"
            },
            {
              "const": "md"
            },
            {
              "const": "html"
            }
          ]
        },
        "title": {
          "type": "string"
        },
        "body": {
          "type": "string"
        },
        "files": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/ObjectPrimitive.IFile"
          }
        },
        "secret": {
          "type": "boolean"
        },
        "created_at": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "extension",
        "title",
        "body",
        "files",
        "secret",
        "created_at"
      ],
      "additionalProperties": false
    },
    "ObjectPrimitive.IFile": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "extension": {
          "type": "string"
        },
        "url": {
          "type": "string"
        },
        "created_at": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "name",
        "extension",
        "url",
        "created_at"
      ],
      "additionalProperties": false
    }
  }
}