{
  "private": true,
  "name": "@typia/website",
  "version": "0.1.0",
  "description": "Typia Guide Documents",
  "scripts": {
    "build": "npm run prebuild && next build && npm run postbuild",
    "prebuild": "rimraf .next && rimraf out && npm run build:compiler && npm run build:typedoc",
    "postbuild": "npm run build:pagefind && npm run build:sitemap",
    "build:compiler": "node build/compiler",
    "build:pagefind": "pagefind --site .next/server/app --output-path out/_pagefind",
    "build:sitemap": "node build/sitemap",
    "build:tgz": "node build/tgz",
    "build:typedoc": "node build/typedoc",
    "deploy": "node build/deploy",
    "dev": "next dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samchon/typia"
  },
  "author": "Jeongho Nam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samchon/typia/issues"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@monaco-editor/react": "^4.6.0",
    "@mui/icons-material": "^5.15.4",
    "@mui/material": "^5.12.0",
    "@rollup/browser": "4.13.0",
    "embed-typescript": "^1.1.0",
    "lz-string": "^1.5.0",
    "monaco-editor": "^0.50.0",
    "next": "^15.3.4",
    "nextra": "^4.6.0",
    "nextra-theme-docs": "^4.6.0",
    "path": "^0.12.7",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tgrid": "^1.0.3",
    "tstl": "^3.0.0",
    "@typia/interface": "./packages/interface.tgz",
    "@typia/core": "./packages/core.tgz",
    "@typia/transform": "./packages/transform.tgz",
    "typia": "./packages/typia.tgz"
  },
  "devDependencies": {
    "@rspack/cli": "^1.0.0",
    "@rspack/core": "^1.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^18.11.10",
    "@types/prettier": "^3.0.0",
    "@types/react": "^18.0.35",
    "gh-pages": "^5.0.0",
    "next-sitemap": "^4.2.3",
    "pagefind": "^1.3.0",
    "process": "^0.11.10",
    "rimraf": "^5.0.0",
    "ts-loader": "^9.5.1",
